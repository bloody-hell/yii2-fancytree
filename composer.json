{
    "name": "bloody-hell/yii2-fancytree",
    "description": "Yii2 wrapper for JS Fancytree widget",
    "minimum-stability": "dev",
    "license": "proprietary",
    "type": "extension",
    "authors": [
        {
            "name": "Ivan Poponin",
            "email": "juf.halli@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "2.0.0-beta",
        "mar10/fancytree": "2.0"
    },
    "repositories" : [
        {
            "type":"package",
            "package":{
                "name":"mar10/fancytree",
                "version":"2.0",
                "source":{
                    "type":"git",
                    "url":"https://github.com/mar10/fancytree",
                    "reference":"master"
                }
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "bloody_hell\\yii2_fancytree\\": ""
        }
    }
}
